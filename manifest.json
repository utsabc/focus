{
  "manifest_version": 3,
  "name": "Focus",
  "version": "1.0",
  "author": "Utsab Chowdhury",
  "description": "Simple chrome extension to focus for reading the web",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
